includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    paths:
        - app/

    # Level 9 is the highest level
    level: 5

    ignoreErrors:
    - '#Call to an undefined method [a-zA-Z0-9\\_]+::getRoleNames()#'
    - '#Call to an undefined method [a-zA-Z0-9\\_]+::getPermissionNames()#'
    - '#Access to an undefined property [a-zA-Z0-9\\_]+::\$[a-zA-Z0-9\\_]#'
